[user]
	name = =our name=
	email = =email=
[color]
    branch = auto
    diff = auto
    status = auto
	ui = true
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red
    new = cyan
[color "status"]
    added = yellow
    changed = green
    untracked = cyan[pack]
	threads = 2
[core]
	autocrlf = false
	editor = vim
[merge]
	tool = vimdiff
[diff]
	tool = vimdiff
[rebase]
	autosquash = true
[branch]
	autosetuprebase = always
[github]
	user = =our username=
	name = =our name=
	email = =email=
[alias]
	ad = add
	aa = add .
	ch = checkout
	co = commit
	ca = commit -a
	st = status -sb
	br = branch
	fe = fetch --all --tags --prune
	pl = pull
	rb = rebase
	ps = push
	au = !git add $(git ls-files -o --exclude-standard)
	lg = log --graph --date=local --format=format:'%C(bold red)%h%Creset %Cgreen%d%Creset %C(yellow)%s%Creset%n         %C(bold white)Author:%Creset  %an%n         %C(bold white)Date:    %Creset%ar (%ad)'